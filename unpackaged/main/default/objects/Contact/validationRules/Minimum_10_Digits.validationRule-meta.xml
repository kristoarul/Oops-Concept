<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Minimum_10_Digits</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Phone)),
    LEN(Phone) &lt; 10
)</errorConditionFormula>
    <errorMessage>TO Check 10 Digits Number</errorMessage>
</ValidationRule>
